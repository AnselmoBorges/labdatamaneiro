{
	"name": "DataLabRescue",
	"properties": {
		"description": "Laboratório de ingestão de arquivo e transformação usando o Azure Data Factory",
		"activities": [
			{
				"name": "Lendo o arquivo",
				"description": "Vai ler o arquivo CSV no storage Account onde o mesmo foi disposto",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				}
			}
		],
		"annotations": []
	}
}